{
  "name": "club-hub-web",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "preview": "vite preview",
    "build": "vite build",
    "build:cf": "esbuild functions/worker.ts --bundle --minify --platform=browser --target=es2022 --format=esm --outfile=dist/_worker.js && cp functions/_routes.json dist/",
    "dev:cf": "wrangler pages dev ./dist",
    "dev:firebase": "firebase emulators:start --import .firebase-emulator-data --export-on-exit",
    "deploy:firebase": "firebase deploy --only firestore",
    "deploy:cf": "npm run build && npm run build:cf && wrangler pages deploy"
  },
  "dependencies": {
    "vue": "^3.4.29",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/vite": "^4.0.1",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/tsconfig": "^0.5.1",
    "esbuild": "^0.24.2",
    "firebase": "^11.0.1",
    "jose": "^5.9.6",
    "tailwindcss": "^4.0.0",
    "typescript": "~5.4.0",
    "vite": "^5.4.14",
    "vue-tsc": "^2.0.21",
    "zod": "^3.23.8"
  }
}
